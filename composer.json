{
  "name": "laralog-app/laralog",
  "description": "A package to work with laralog.app to allow easy access to your app's logs.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Laralog",
      "email": "contact@laralog.app"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laralog\\Laralog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laralog\\Laralog\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laralog\\Laralog\\LaralogServiceProvider"
      ]
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "laravel/framework": ">=5.6",
    "jalle19/php-whitelist-check": "^1.0",
    "barryvdh/laravel-cors": "^0.11"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "~3.6",
    "phpunit/phpunit": "~7.0"
  }
}
